{
  "version": 2,
  "routes": [
    {
      "src": "/build/(.*)",
      "dest": "/public/build/$1",
      "continue": true
    },
    {
      "src": "/css/(.*)",
      "dest": "/public/css/$1",
      "continue": true
    },
    {
      "src": "/js/(.*)",
      "dest": "/public/js/$1",
      "continue": true
    },
    {
      "src": "/images/(.*)",
      "dest": "/public/images/$1",
      "continue": true
    },
    {
      "src": "/img/(.*)",
      "dest": "/public/img/$1",
      "continue": true
    },
    {
      "src": "/fonts/(.*)",
      "dest": "/public/fonts/$1",
      "continue": true
    },
    {
      "src": "/favicon.ico",
      "dest": "/public/favicon.ico",
      "continue": true
    },
    {
      "src": "/robots.txt",
      "dest": "/public/robots.txt",
      "continue": true
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.php"
    }
  ]
}